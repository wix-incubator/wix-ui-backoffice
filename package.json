{
  "private": false,
  "name": "wix-ui-backoffice",
  "description": "Components for Wix backoffice applications",
  "version": "1.0.0",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "files": [
    "dist",
    "*.js",
    "*.d.ts",
    "!wallaby.js",
    "!protractor.conf.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/wix-ui-backoffice.git"
  },
  "scripts": {
    "build": "yoshi build && import-path --path src/components --dts",
    "pretest": "build-storybook",
    "test": "npm run test:unit && npm run test:e2e",
    "posttest": "npm run lint",
    "test:watch": "yoshi test --jest --watch",
    "test:unit": "yoshi test --jest",
    "test:e2e": "yoshi test --protractor",
    "lint": "yoshi lint",
    "start": "yoshi start --no-test & npm run storybook",
    "release": "yoshi release && gh-pages-auto-release --dist=storybook-static",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "wix-ui-core": "latest",
    "wix-ui-theme": "latest"
  },
  "devDependencies": {
    "@storybook/addon-options": "^3.3.7",
    "@storybook/react": "^3.3.7",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.1",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.8",
    "@types/react-dom": "^16.0.5",
    "@types/react-onclickoutside": "^6.0.2",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "eyes.it": "latest",
    "gh-pages-auto-release": "latest",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "latest",
    "prop-types": "^15.6.0",
    "protractor": "^5.3.0",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.3.1",
    "surge-github-autorelease": "^1.0.23",
    "ts-jest": "^22.0.3",
    "tslint-react": "^3.5.1",
    "typescript": "^2.6.2",
    "wait-for-cond": "^1.5.1",
    "wix-storybook-utils": "latest",
    "wix-ui-icons-common": "latest",
    "wix-ui-test-utils": "latest",
    "yoshi": "^1.2.0"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/src/.*\\.spec\\.(ts|tsx)$",
    "setupTestFrameworkScriptFile": "wix-ui-test-utils/jest-setup",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme-setup.js"
    ]
  }
}
