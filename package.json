{
  "private": false,
  "name": "wix-ui-backoffice",
  "description": "Components for Wix backoffice applications",
  "version": "1.0.0",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "files": [
    "dist",
    "src",
    "*.js",
    "*.d.ts",
    "!wallaby.js",
    "!protractor.conf.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/wix-ui-backoffice.git"
  },
  "scripts": {
    "build": "yoshi build && import-path --path src/components --dts && build-storybook",
    "test": "npm run test:unit && npm run test:e2e",
    "posttest": "npm run lint",
    "test:watch": "yoshi test --jest --watch",
    "test:unit": "yoshi test --jest",
    "test:e2e": ":",
    "lint": "yoshi lint",
    "start": "yoshi start --no-test & npm run storybook",
    "release": "yoshi release",
    "postpublish": "npm install teamcity-surge-autorelease@^1.0.0 --no-save && teamcity-surge-autorelease --dist=storybook-static",
    "storybook": "start-storybook -p 6006",
    "storybook:static": "start-storybook -p 6006 -s storybook-static",
    "pr-postbuild": "npm install teamcity-surge-autorelease@^1.0.0 --no-save && teamcity-surge-autorelease --dist=storybook-static"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "lodash": "^4.17.5",
    "shallowequal": "^1.1.0",
    "wix-ui-core": "^2.0.0",
    "wix-ui-icons-common": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-links": "4.0.0-alpha.14",
    "@storybook/addon-options": "4.0.0-alpha.14",
    "@storybook/react": "4.0.0-alpha.14",
    "@stylable/dom-test-kit": "^0.1.0",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.107",
    "@types/node": "^8.0.0",
    "@types/react": "^16.4.2",
    "@types/react-dom": "^16.0.5",
    "@types/react-onclickoutside": "^6.0.2",
    "@types/shallowequal": "^0.2.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "eyes.it": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "latest",
    "protractor": "^5.3.0",
    "raf": "^3.4.0",
    "react": "~16.4.2",
    "react-dom": "~16.4.2",
    "react-test-renderer": "~16.4.2",
    "ts-jest": "^22.0.3",
    "tslint-config-prettier": "^1.12.0",
    "tslint-react": "^3.5.1",
    "typescript": "~3.0.3",
    "wait-for-cond": "^1.5.1",
    "wix-eventually": "^2.2.0",
    "wix-storybook-utils": "^2.0.0",
    "wix-ui-test-utils": "^1.0.93",
    "yoshi": "^2.13.3"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/src/.*\\.spec\\.(ts|tsx)$",
    "setupTestFrameworkScriptFile": "wix-ui-test-utils/jest-setup",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme-setup.js"
    ]
  }
}
